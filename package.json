{
  "name": "gasless-cctp",
  "version": "1.0.0",
  "description": "Automated & Gasless Cross Chain Transfer Protocol",
  "scripts": {
    "build": "yarn install && yarn compile && npx tsc",
    "clean": "yarn hardhat clean && rm -rf dist",
    "compile": "npx hardhat compile --force",
    "deploy": "npx hardhat deploy",
    "test": "npx hardhat test",
    "format": "prettier --write '*/**/*.*{js,json,md,ts}'",
    "format:check": "prettier --check '*/**/*.*{js,json,md,ts}'",
    "lint": "eslint --cache . && yarn lint:sol",
    "lint:ts": "eslint -c .eslintrc.json --ext \"**/*.ts\" \"**/*.test.ts\"",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "postinstall": "yarn husky install",
    "bridge": "ts-node --project ./tsconfig.node.json scripts/bridge_new.ts",
    "task": "ts-node --project ./tsconfig.node.json scripts/create-task.ts",
    "w3f:test": "npx w3f test web3-functions/maple-cctp/index.ts --logs --chain-id=11155111"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@gelatonetwork/automate-sdk": "3.0.10",
    "@gelatonetwork/relay-context": "^4.1.1",
    "@gelatonetwork/relay-sdk": "5.5.6",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "1.0.12",
    "@nomiclabs/hardhat-etherscan": "3.1.8",
    "@openzeppelin/contracts": "4.9.3",
    "@tsconfig/recommended": "1.0.3",
    "@typechain/ethers-v6": "0.5.1",
    "@typechain/hardhat": "9.1.0",
    "@types/chance": "1.1.3",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "chai": "4.3.7",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "ethers": "6.11.1",
    "hardhat": "2.22.14",
    "hardhat-deploy": "0.12.2",
    "husky": "8.0.3",
    "ky": "1.2.2",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-solidity": "1.3.1",
    "solhint": "4.5.2",
    "solhint-plugin-prettier": "0.1.0",
    "ts-node": "10.9.2",
    "typechain": "8.3.2",
    "typescript": "5.4.2"
  },
  "lint-staged": {
    "*.*{js,json,md,ts,sol,yml,yaml}": "prettier --write",
    "*.*{ts,js}": "eslint -c .eslintrc.json"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "dependencies": {
    "@gelatonetwork/web3-functions-sdk": "^2.4.4"
  }
}
